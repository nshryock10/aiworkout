{
    "version": 2,
    "builds": [
      {
        "src": "/backend/app2.js",
        "use": "@vercel/node"
      },
      {
        "src": "./public/**",
        "use": "@vercel/static"
      },
      {
        "src": "./src/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ],
    "rewrites": [
      { 
        "source": "/(.*)", 
        "destination": "/backend/app2.js" 
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }