{
    "version": 2,
    "builds": [
      {
        "src": "./backend/app2.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "backend/.*",
        "dest": "backend/app2.js"
      },
      {
        "src": "build/static/.*",
        "dest": "static/.*"
      },
      {
        "src": "build/index.html",
        "dest": "index.html"
      }
    ]
  }